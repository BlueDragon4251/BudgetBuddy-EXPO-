{
  "expo": {
    "name": "BudgetBuddy",
    "slug": "BudgetBuddy",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
"splash": {
  "image": "./assets/images/splash-icon.png",
  "resizeMode": "contain",
  "backgroundColor": "system"
},
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fb082d04-1b0c-4398-8276-724d12fdf767"
      }
    },
    "owner": "bluedragon42",
    "android": {
      "package": "me.bluedragon.budgetbuddy"
    }
  }
}
